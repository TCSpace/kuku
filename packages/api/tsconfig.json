{
    "extends": "../shared/typescript.json",
    "include": ["./src", "./prisma"],
    "compilerOptions": {
        "rootDirs": ["src"],
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "ES2022",
        "lib": ["ES2022"],
        "types": ["node"],
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/**/*"]
        }
    }
}
